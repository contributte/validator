{
  "name": "contributte/validator",
  "description": "Symfony/Validator integration for Nette Framework.",
  "keywords": [
    "symfony",
    "nette",
    "validator"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/validator",
  "authors": [
    {
      "name": "Jiří Pudil",
      "email": "me@jiripudil.cz",
      "homepage": "https://jiripudil.cz"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/di": "^3.0.9",
    "symfony/cache": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "symfony/config": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "symfony/validator": "^6.4.0 || ^7.0.0 || ^8.0.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.5.0",
    "contributte/tester": "^0.4.0",
    "doctrine/cache": "^2.2.0",
    "nette/bootstrap": "^3.0.0",
    "symfony/translation": "^6.0.0 || ^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Validator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
